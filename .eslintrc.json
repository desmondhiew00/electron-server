{
  "env": {
    "browser": true,
    "es2021": true
  },
  "settings": {
    "import/core-modules": ["electron"]
  },
  "extends": ["plugin:react/recommended", "airbnb"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "quotes": "off",
    "comma-dangle": "off",
    "import/prefer-default-export": "off",
    "no-use-before-define": "off"
  }
}
